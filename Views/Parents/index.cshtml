@model IEnumerable<Pschool.Models.Parent>

<h2>Parents</h2>

<div class="container">
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>Home Address</th>
                <th>Phone1</th>
                <th>Work Phone</th>
                <th>Home Phone</th>
                <th>Siblings</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.Username</td>
                    <td>@item.Email</td>
                    <td>@item.HomeAddress</td>
                    <td>@item.Phone1</td>
                    <td>@item.WorkPhone</td>
                    <td>@item.HomePhone</td>
                    <td>@item.Siblings</td>
                </tr>
            }
        </tbody>
    </table>
</div>